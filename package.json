{
  "name": "netlify-graphql",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "0.1.1",
    "apollo-datasource-rest": "^0.1.0-rc.11",
    "apollo-server-lambda": "^2.0.0-xrc.11",
    "apollo-server-redis": "^0.1.0-rc.11",
    "graphql": "^0.13.2",
    "lodash": "^4.17.10",
    "netlify-lambda": "^0.4.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.6",
    "react-art": "16.3.2",
    "react-dom": "^16.4.1",
    "react-native-web": "latest",
    "react-router": "latest",
    "react-router-dom": "latest",
    "react-scripts": "1.1.4",
    "recyclerlistview": "1.2.6",
    "shorthash": "0.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:lambda": "netlify-lambda serve -c lambda.webpack.config.js api",
    "build": "react-scripts build",
    "build:lambda": "netlify-lambda build -c lambda.webpack.config.js api",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
